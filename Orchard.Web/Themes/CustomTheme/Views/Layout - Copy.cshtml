					$('#span_visitorinfo_inner').show();
					$('#span_visitorinfo_outer').hide();
					$('#loginwin').hide();
					$('#anchor_signout').show();
				} else {
					$('#span_visitorinfo_inner').hide();
					$('#span_visitorinfo_outer').show();
					$('#loginwin').show();
					$('#anchor_signout').hide();
				}
			}

