			    var id = obj.attr("rel");
			    $('#' + id).show();
			    obj.addClass("selected");
			}

			function pager_click(index) {
				var currentPageIndex = catalogueListRequest.getCurrentPageIndex();
				var lastPageIndex = $("#spanTopLastPageIndex").text();
				if(index === 1) {
					if(currentPageIndex != 0) {
						catalogueListRequest.setCurrentPageIndex(0);
						AppViewModel.productList.loadCatalogueList();
					}
				} else if(index === 2) {
					if(currentPageIndex > 0) {
						currentPageIndex = currentPageIndex - 1;
						catalogueListRequest.setCurrentPageIndex(currentPageIndex);
						AppViewModel.productList.loadCatalogueList();
					}
				} else if(index === 3) {
					if(currentPageIndex < (lastPageIndex - 1)) {
						currentPageIndex = currentPageIndex + 1;
						catalogueListRequest.setCurrentPageIndex(currentPageIndex);
						AppViewModel.productList.loadCatalogueList();
					}
				} else if(index === 4) {
					if(currentPageIndex < (lastPageIndex - 1)) {
						catalogueListRequest.setCurrentPageIndex(lastPageIndex - 1);
						AppViewModel.productList.loadCatalogueList();
					}
				}
			}

			function refreshContent(value) {
				AppViewModel.productList.loadCatalogueList();
