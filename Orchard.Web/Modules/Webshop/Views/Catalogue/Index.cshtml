				<span>Belső raktár:</span> ${item.InnerStock}
				<br/><span>Külső raktár:</span> ${item.OuterStock}
				{{else item.PurchaseInProgress && Visitor.IsValidLogin}}
				<span>Várható érkezés:</span>${ShippingDate}
				{{else Visitor.IsValidLogin}}
				<span>Rendelhető</span>
				{{/if}}
				{{if item.Bargain && Visitor.IsValidLogin}}
				<br /><span>Használt: 4 600 Ft-tól </span>
				{{/if}}
				{{if Visitor.IsValidLogin}}
				<br /><span>Garancia módja:<br/> ${item.GarantyMode}</span>
				<br /><span>Garancia ideje: ${item.GarantyTime}</span>
				{{/if}}
				{{if item.Special && Visitor.IsValidLogin}}
				<br/><span>hírlevél akció</span>
				{{/if}}
				{{if item.New && Visitor.IsValidLogin}}
				<br/><span>új termék</span>
				{{/if}}
				{{if item.Discount && Visitor.IsValidLogin}}
				<br/><span>leértékelt</span>
				{{/if}}
				{{if item.ItemState == 1 && Visitor.IsValidLogin}}
				<br/><span>kifutó</span>
				{{/if}}</div>
				</td>
				</tr>

				</table></td>
				</tr>
				<tr>
				<td colspan="3" valign="top" class="cus_prod_table_more">
				<table width="100%" border="0" cellspacing="1" cellpadding="0">
				<tr>
				<td nowrap="nowrap">
				<div class="cus_prod_table_morelink">
				<a href="@Url.Content("~/Webshop/Catalogue/")${item.ProductId}/Details" title="Részletes termékadatlap" target="_blank">[+] Részletes adatlap</a>
				</div></td>
				<td nowrap="nowrap">
				<div class="cus_prod_table_add_compare">
				<input type="checkbox" name="checkbox" id="checkbox" />
				</div></td>
				<td width="100%"> Összehasonlító listához ad </td>
				<td nowrap="nowrap">
				<div class="cus_prod_table_quantity">
